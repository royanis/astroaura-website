# Environment variables for AstroAura Website
# Copy this file to .env and update with your values

# Google Analytics
GOOGLE_ANALYTICS_ID=

# GitHub Pages
GITHUB_REPOSITORY=astroaura/website
GITHUB_BRANCH=main

# Lighthouse CI
LHCI_GITHUB_APP_TOKEN=

# Development
JEKYLL_ENV=development
NODE_ENV=development

# API Keys (for future use)
ASTROLOGY_API_KEY=
WEATHER_API_KEY=

# Social Media
TWITTER_USERNAME=AstroAuraApp
GITHUB_USERNAME=astroaura

# Contact
CONTACT_EMAIL=contact@astroaura.me
SUPPORT_EMAIL=support@astroaura.me